{
    "manifest_version": 3,
    "name": "Autofill MDAC",
    "description": "Autofill Malaysia Digital Arrival Card",
    "version": "2.2.2",
    "content_scripts": [
        {
            "matches": [
                "https://imigresen-online.imi.gov.my/mdac/main?registerMain"
            ],
            "js": [
                "scripts/jquery.min.js",
                "scripts/content.js"
            ]
        }
    ],
    "options_page": "options.html",
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    }
}